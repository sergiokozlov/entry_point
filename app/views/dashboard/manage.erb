

  <div class="clear"></div>

  <div class="report weekly">
    <h2>My Team Weekly Stats </h2>
    <br/>
	<table cellspacing="0">
        <thead>
            <th>Team Member</th>
			<th>Week</th>
			<th># Working Days</th>
			<th># Hours Spent</th>
			<th>Daily Average</th>
		</thead>
        <tbody>
          <%  @user.developers.each do |dev| %>
			<% dev.logged_working_weeks.each do |lww| %>
            <tr class = "<%= cycle("alt","white") -%>">
                <td> <%=dev.name %> </td>
				<td> <%= "#{week_first_day(lww[0]).to_s} - #{week_last_day(lww[0])}" %></td>
                <td> <%=dev.weeked_working_days(lww[0]).length%> </td>
				<td><%=to_time(week_completed(lww[0],dev))%> </td>
				<td><%=to_time(week_average(lww[0],dev))%></td>
			</tr>
            <%end%>
          <% end %>
		</tbody>
	</table>
  </div>
  

