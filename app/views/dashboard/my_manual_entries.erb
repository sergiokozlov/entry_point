	<table id="manual_entries" cellspacing="0">
		<thead>
			<tr>
				<th>Entry Type</th>
                <th>Time</th>
                <th></th>
                <!-- <th><img src="/images/bar-chart.png" alt="" /></th> -->
			</tr>
          </thead>
          <tbody>
              <%@wd.records.select {|r| r.submit_type == 'manual'}.each do |mr| %> 
              <tr class = "<%= cycle("white","alt") -%>">
                  <td> Check-in or Check-out </td>
                  <td> <%= mr.click_date.strftime("%m/%d/%Y %H:%M") %></td>
                  <td> <INPUT TYPE=CHECKBOX NAME="record_<%=mr.id %>"> </td>
              </tr>
             <%end %>
             <%@wd.homework.to_a.each do |h| %> 
              <tr class = "<%= cycle("white","alt") -%>">
                  <td> Homework </td>
                  <td> <%= h.check_in.strftime("%m/%d/%Y %H:%M") %> - <br/>
                       <%= h.check_out.strftime("%m/%d/%Y %H:%M") %>
                  </td>
                  <td> <INPUT TYPE=CHECKBOX NAME="homework_<%=h.id %>">  </td>
              </tr>
              <%end %>
          </tbody>
	</table>

