  <h2> Use the form below to submit missed entry record </h2>
	<br/>
    <% form_for @record,:html => {:class => 'input_form'}  do |f| %>
      <%= f.error_messages %>

<p>
	<%=    f.text_field :click_date_string, :placeholder =>  "2010-09-23 11:30" %>
    <%=   f.label :click_date, "Check In" %>
</p>

   <%=    f.submit "Add Record" %>
 <% end %>

